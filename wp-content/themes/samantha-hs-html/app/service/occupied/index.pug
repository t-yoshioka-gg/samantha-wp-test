extends /inc/foundation/_base.pug
block append config
  - current.id = "occupied" // ページのID
  - current.title = "居住中ホームステージング" // タイトル
  - current.description = "これは居住中ホームステージングについての説明文です" // 説明文
  - current.bodyClass = "occupied" // body に付与するクラス
  - current.depth = 3 // ページの階層

block page_header
  +l_page_header({
    image: "img-page-header-occupied.jpg",
    title: "居住中ホームステージング",
    subtitle: "サービス"
  })
  +c_breadcrumb("サービス","service")

block body
  main.l-main
    +c_floating-contact("/contact/?service=occupied")
    section.l-section.is-md
      .l-container
        +c.service-page-lead
          +e.links
            +c_capsul-nav([
              {
                id: "#occupied",
                text: `居住中ホームステージング`,
              },
              {
                id: "#comic",
                text: `漫画で読むホームステージング`,
              },
            ])
          +e.lead#occupied
            +c_page-lead-block({
              subtitle: `OCCUPIED HOME STAGING`,
              title: `居住中ホームステージング`,
              text: `居住中物件専門のホームステージャーによる、「住みながら売るための空間演出」サービスです。<br/>売主様のお住まいを片付けた後、お持ちのインテリア小物などをお借りして、モデルルームのように飾り付け。<br/>生活感を抑えつつ、物件本来の魅力・価値が伝わるように演出を施すことで、早期売却をサポートします。`
            })
        .row
          .large-10.is-push-lg-1.small-12
            +c.block-iframe.u-mbs.is-sm.is-top
              |<iframe width="1110" height="625" src="https://www.youtube.com/embed/1_lFh7mVj1k" title="【ビフォーアフター】居住中ホームステージング" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>

    section.l-section.is-bg-color.is-md
      .l-container
        h2.c-heading.is-md 空室ホームステージング実例

      +c.gallery-slider
        +e.inner
          +e.main
            +e.main-content
              +e.main-image: +bgimg("img-gallery-occupied-01.jpg").c-gallery-slider__main-bgimg
              +e.text ①リビング〈ホームステージング前〉
            +e.main-content
              +e.main-image: +bgimg("img-gallery-occupied-02.jpg").c-gallery-slider__main-bgimg
              +e.text ①リビング〈ホームステージング後〉
            +e.main-content
              +e.main-image: +bgimg("img-gallery-occupied-03.jpg").c-gallery-slider__main-bgimg
